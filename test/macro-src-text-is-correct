#!/bin/bash
#
# Ensure that ASTs generated by a macro have correct source test.
#
. $(dirname $0)/common

equals "$(run_macros -json | json_filter counter 3 | json -e src_text -u)" \
       "MULTI_STATEMENT_1"

equals "$(run_macros -json | json_filter counter 8 | json -e src_text -u)" \
       "MULTI_STATEMENT_2(foo)"
